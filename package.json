{
  "name": "argos",
  "version": "3.11.0",
  "description": "OB/Flypack Monitoring",
  "main": "main.js",
  "type": "module",
  "dependencies": {
    "aws-sdk": "^2.1450.0",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "net-snmp": "^3.9.7",
    "node-fetch": "^2.6.7",
    "ping": "^0.4.4",
    "source-map-support": "^0.5.21",
    "ws": "^8.13.0",
    "xeue-config": "^2.2.2",
    "xeue-logs": "^2.3.1",
    "xeue-sql": "^2.0.6",
    "xeue-webserver": "^2.0.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder --win"
  },
  "build": {
    "appId": "com.xeue.argos",
    "productName": "Argos",
    "files": [
      "**/*",
      "static/*"
    ],
    "win": {
      "icon": "static/img/icon/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "artifactName": "Argos-v${version}.exe"
    }
  },
  "author": "Sam Chilton & Ollie Quirke",
  "license": "MIT"
}
